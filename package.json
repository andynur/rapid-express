{
  "name": "rapid-express",
  "version": "0.0.0",
  "description": "TypeScript + Prisma + MySQL + Express API Server",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "NODE_ENV=development bun run src/server.ts",
    "dev": "NODE_ENV=development bun --watch run src/server.ts",
    "build": "bun build src/server.ts --outdir ./dist",
    "test": "bun test",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "bun lint --fix",
    "prisma:init": "prisma init",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:seed": "bun run src/prisma/seed.ts",
    "prisma:generate": "prisma generate",
    "deploy:prod": "bun build && NODE_ENV=production bun run src/server.ts",
    "deploy:dev": "NODE_ENV=development bun --watch run src/server.ts",
    "prepare": "husky",
    "precommit": "eslint ."
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^4.1.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.5.0",
    "typedi": "^0.10.0",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^2.7.1",
    "prisma": "^4.1.0"
  }
}
